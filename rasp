        bot.send_message(message.chat.id, '‚ñπ2 –ø–∞—Ä–∞ (10:25-12:00) - –û—Å–Ω–æ–≤—ã —ç–∫–æ–ª–æ–≥–æ-—ç–Ω–µ—Ä–≥–µ—Ç–∏—á–µ—Å–∫–æ–π —É—Å—Ç–æ–π—á–∏–≤–æ—Å—Ç–∏ –ø—Ä–æ–∏–∑–≤–æ–¥—Å—Ç–≤–∞ - –∞.237', reply_markup=keyboard4)
        bot.send_message(message.chat.id, '‚ñπ3 –ø–∞—Ä–∞ (12:30-14:05) - –§–∏–∑–∏–∫–∞ - –∞.307/2', reply_markup=keyboard4)
        bot.send_message(message.chat.id, '‚ñπ4 –ø–∞—Ä–∞ (14:20-15:55) - –§–∏–∑–∏—á–µ—Å–∫–∞—è –∫—É–ª—å—Ç—É—Ä–∞', reply_markup=keyboard4)

    elif message.text.lower() == '—Å–ø–∞—Å–∏–±–æ':
        bot.send_sticker(message.chat.id, 'CAACAgIAAxkBAAEBSHpfTqjRjBGnEyuP4r8n2N7UdrVllgACiioAAulVBRgcqZWVyQ5Y_BsE', reply_markup=keyboard1)

